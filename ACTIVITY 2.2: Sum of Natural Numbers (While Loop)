import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int n = sc.nextInt();
        int i = 1, sum = 0;
        do {
            System.out.println(i);
            sum += i;
            i++;
        } while (i <= n);
        System.out.println(sum);
        sc.close();
    }
}
